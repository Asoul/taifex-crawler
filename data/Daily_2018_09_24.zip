<script type="text/javascript">

	
  var tradeDate = new Date(); 
  var timeStart = new Date();
  var reflashSeconds = 40;
  timeStart.setHours(00,00,00);
  
  var timeEnd = new Date(); 
  timeEnd.setHours(23,59,00);

 

  if (tradeDate >=timeStart && tradeDate <= timeEnd ) {
	  var diff = timeEnd - tradeDate;
	  var secs = Math.floor(diff/1000);
	  
	  var clockproduct = window.setInterval("clockproductgo()", reflashSeconds * 1000);
	  var clockproductstop = window.setInterval("clockproductclear()", secs * 1000);
  } else if (tradeDate < timeStart) {
	  var diff = timeEnd - timeStart;
		var secs = Math.floor(diff/1000);
	  function clockproductrestart()
		{
			clockproductgo();
			clearInterval(clockproduct);
			clockproduct = window.setInterval("clockproductgo()", reflashSeconds * 1000);
			clockproductstop = window.setInterval("clockproductclear()", secs * 1000);
		}
	var clockproduct = window.setInterval("clockproductrestart()", secs * 1000);
	var clockproductstop;
  }

  
</script>
